@model IEnumerable<Make>
<div class="dropdown-menu">
    <a class="dropdown-item text-dark" asp-area="" asp-page="/Cars/Index" asp-route-makeId="">All</a>

    @foreach (var item in Model)
    {
        <a class="dropdown-item text-dark" asp-page="/Cars/Index" asp-route-makeId="@item.Id">@item.Name</a>
    }
</div>